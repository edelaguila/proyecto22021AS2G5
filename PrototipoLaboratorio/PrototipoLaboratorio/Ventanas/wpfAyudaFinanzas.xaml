<Window x:Class="PrototipoLaboratorio.Ventanas.wpfAyudaFinanzas"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PrototipoLaboratorio.Ventanas"
        mc:Ignorable="d"
        Title="wpfAyudaFinanzas" Height="750" Width="1310">
    <Grid>
        <Grid x:Name="Gestión_de_Aseguradoras" Margin="0,10,0,-10">
            <Grid.Background>
                <ImageBrush ImageSource="/Ventanas/fondo_2.jpg"/>
            </Grid.Background>
            <Label x:Name="lblDescripcion" Content="Ver archivo de ayuda:" HorizontalAlignment="Left" Margin="153,102,0,0" VerticalAlignment="Top" Height="38" Width="364" FontSize="14" FontFamily="Century Gothic" VerticalContentAlignment="Center"/>
            <Button Content="Ayuda" HorizontalAlignment="Left" Height="120" Margin="153,172,0,0" VerticalAlignment="Top" Width="309" Click="Button_Click"/>
        </Grid>

    </Grid>
</Window>
